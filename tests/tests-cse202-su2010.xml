    <assignment id="pa1" name="PA 1">
        <test name="Test 1">
            <io>
                <prompt>Enter number of seconds: </prompt>
                <input>0</input>
                <output>0 days, 0 hours, 0 minutes, 0 seconds from now.</output>
            </io>
        </test>
        <test name="Test 2">
            <io>
                <prompt>Enter number of seconds: </prompt>
                <input>55</input>
                <output>0 days, 0 hours, 0 minutes, 55 seconds from now.</output>
            </io>
        </test>
        <test name="Test 3">
            <io>
                <prompt>Enter number of seconds: </prompt>
                <input>-55</input>
                <output>0 days, 0 hours, 0 minutes, 55 seconds ago.</output>
            </io>
        </test>
        <test name="Test 4">
            <io>
                <prompt>Enter number of seconds: </prompt>
                <input>1000</input>
                <output>0 days, 0 hours, 16 minutes, 40 seconds from now.</output>
            </io>
        </test>
        <test name="Test 5">
            <io>
                <prompt>Enter number of seconds: </prompt>
                <input>-1000</input>
                <output>0 days, 0 hours, 16 minutes, 40 seconds ago.</output>
            </io>
        </test>
        <test name="Test 6">
            <io>
                <prompt>Enter number of seconds: </prompt>
                <input>12345678</input>
                <output>142 days, 21 hours, 21 minutes, 18 seconds from now.</output>
            </io>
        </test>
        <test name="Test 7">
            <io>
                <prompt>Enter number of seconds: </prompt>
                <input>-12345678</input>
                <output>142 days, 21 hours, 21 minutes, 18 seconds ago.</output>
            </io>
        </test>
    </assignment>
    
