    <assignment id="lab2" name="Lab 2">
        <test name="Test 1">
            <io>
                <prompt>Enter non-zero integers a, b, and real c: </prompt>
                <input>1 1 1</input>
                <output>
Quotient a/b: 1
Remainder of a/b: 0
a^3: 1
Division c/a: 1.000
                </output>
            </io>
        </test>
        <test name="Test 2">
            <io>
                <prompt>Enter non-zero integers a, b, and real c: </prompt>
                <input>1 2 3</input>
                <output>
Quotient a/b: 0
Remainder of a/b: 1
a^3: 1
Division c/a: 3.000
                </output>
            </io>
        </test>
        <test name="Test 3">
            <io>
                <prompt>Enter non-zero integers a, b, and real c: </prompt>
                <input>-1 2 -3.5</input>
                <output>
Quotient a/b: 0
Remainder of a/b: -1
a^3: -1
Division c/a: 3.500
                </output>
            </io>
        </test>
        <test name="Test 4">
            <io>
                <prompt>Enter non-zero integers a, b, and real c: </prompt>
                <input>73493 -23472 347.34457656</input>
                <output>
Quotient a/b: -3
Remainder of a/b: 3077
a^3: -1823844051
Division c/a: 0.005
                </output>
            </io>
        </test>
    </assignment>
    
    <assignment id="lab3" name="Lab 3">
        <test name="Test 1 (add)">
            <io>
                <prompt>Enter a simple mathematical formula: </prompt>
                <input>0.0 + 0.0</input>
                <output>Answer: 0.000</output>
            </io>
        </test>
        <test name="Test 2 (subtract)">
            <io>
                <prompt>Enter a simple mathematical formula: </prompt>
                <input>123.45 - 123.45</input>
                <output>Answer: 0.000</output>
            </io>
        </test>
        <test name="Test 3 (multiply)">
            <io>
                <prompt>Enter a simple mathematical formula: </prompt>
                <input>5.67 * -84.27</input>
                <output>Answer: -477.811</output>
            </io>
        </test>
        <test name="Test 4 (divide)">
            <io>
                <prompt>Enter a simple mathematical formula: </prompt>
                <input>-3497.45 / 354.3385045</input>
                <output>Answer: -9.870</output>
            </io>
        </test>
        <test name="Test 5 (divide by zero)">
            <io>
                <prompt>Enter a simple mathematical formula: </prompt>
                <input>35 / 0</input>
                <output>Answer: inf</output>
            </io>
        </test>
        <test name="Test 6 (not a number)">
            <io>
                <prompt>Enter a simple mathematical formula: </prompt>
                <input>0.0 / 0.0</input>
                <output>Answer: nan</output>
            </io>
        </test>
    </assignment>
    
    <assignment id="lab4" name="Lab 4">
        <test name="Test 1">
