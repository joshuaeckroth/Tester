                <output>Answer: 0.000</output>
            </io>
            <io>
                <prompt>Enter a simple mathematical formula: </prompt>
                <input>0 - 0</input>
                <output>Answer: 0.000</output>
            </io>
            <io>
                <prompt>Enter a simple mathematical formula: </prompt>
                <input>0 / 0</input>
                <output>Done.</output>
            </io>
        </test>
        <test name="Test 3">
            <io>
                <prompt>Enter a simple mathematical formula: </prompt>
                <input>9 * 4</input>
                <output>Answer: 36.000</output>
            </io>
            <io>
                <prompt>Enter a simple mathematical formula: </prompt>
                <input>87 + 0.2</input>
                <output>Answer: 87.200</output>
